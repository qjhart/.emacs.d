    (blue-2fg . "#a9ffff")
    (cyan-2fg . "#a8ff90")
    (green-2fg . "#ffff93"))
  '((custom-theme-set-faces theme-name
			    `(org-block-begin-line
			      ((,class
				(:foreground ,(solarized-color-blend base03 base3 0.05 2)
					     :background ,(solarized-color-blend base03 base3 0.95 2)))))
			    `(org-block
			      ((,class
				(:background ,(solarized-color-blend base03 base3 0.95 2)))))
			    `(org-block-end-line
			      ((,class
				(:foreground ,(solarized-color-blend base03 base3 0.05 2)
					     :background ,(solarized-color-blend base03 base3 0.95 2))))))
    `(mode-line
      ((,class
	(:foreground ,base2 :background ,(solarized-color-blend base03 base3 0.5 2)))))
    `(mode-line-inactive
      ((,class
	(:foreground ,base00 :background ,(solarized-color-blend base03 "black" 0.85 2)))))
    `(mode-line-buffer-id
      ((,class
	(:foreground ,base3 :weight bold))))))
(provide-theme 'solarized-ucd-light)
(provide 'solarized-ucd-light-theme)
